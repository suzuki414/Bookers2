<h4>Users</h4>
<table class="table table-striped">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= image_tag @user.get_profile_image(80,80) %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show", user_path(user) %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= render 'users/show', user: @user %>
<%= render 'books/new', book: @book %>